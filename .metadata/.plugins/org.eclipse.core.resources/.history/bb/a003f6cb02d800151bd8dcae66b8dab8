#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

class Person{
private:
	string name;
	int age;
public:
	virtual void getdata() = 0;
	virtual void putdata() = 0;
	void putname(string s){
		strcpy(this->name,s);
	}
};

class Professor: public Person{
private:
	int publications;
	static int prof_id = 0;
public:
	Professor(void){
		prof_id++;
	}
	void getdata(){
		getline(cin,name);
		cin>>age;
		cin>>publications;
	}
	void putdata(){
		cout<<name<<" "<<age<<" "<<publications<<endl;
		cout<<prof_id;
	}
};
class Student: public Person{
private:
	int marks[6];
	static int stud_id = 0;
public:
	Student(void){
		stud_id++;
	}
	void getdata(){
		getline(cin,name);
		cin>>age;
		for(int i = 0; i < 6 ;i++)cin>>marks[i];
	}
	void putdata(){
		int totalmarks = 0;
		for(int i = 0; i <6 ;i++)totalmarks+=marks[i];
	}
};






int main(){

    int n, val;
    cin>>n; //The number of objects that is going to be created.
    Person *per[n];

    for(int i = 0;i < n;i++){

        cin>>val;
        if(val == 1){
            // If val is 1 current object is of type Professor
            per[i] = new Professor;

        }
        else per[i] = new Student; // Else the current object is of type Student

        per[i]->getdata(); // Get the data from the user.

    }

    for(int i=0;i<n;i++)
        per[i]->putdata(); // Print the required output for each object.

    return 0;

}
